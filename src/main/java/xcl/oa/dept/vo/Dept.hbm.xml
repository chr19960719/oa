    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="xcl.oa.dept.vo.Dept" table="dept">
		<id name="deptID">
			<generator class="native"/>
		</id>
		<property name="deptName"/>
		<property name="deptText"/>
	</class>
</hibernate-mapping>